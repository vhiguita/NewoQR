{
  "name": "NewoQR",
  "version": "0.0.1",
  "author": "Ionic Framework + JHipster",
  "homepage": "https://www.jhipster.tech",
  "scripts": {
    "ng": "ng",
    "start": "ionic serve",
    "build": "ng build",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "npm run e2e-update && npm run e2e-test",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "e2e-test": "protractor ./e2e/protractor.conf.js",
    "e2e-update": "node node_modules/webdriver-manager/bin/webdriver-manager update --gecko false",
    "prettier": "prettier --write \"{,src/**/}*.{md,json,ts,css,scss,yml}\" --loglevel silent"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~8.1.2",
    "@angular/compiler": "~8.1.2",
    "@angular/core": "~8.1.2",
    "@angular/forms": "~8.1.2",
    "@angular/platform-browser": "~8.1.2",
    "@angular/platform-browser-dynamic": "~8.1.2",
    "@angular/router": "~8.1.2",
    "@ionic-native/core": "^5.0.0",
    "@ionic-native/splash-screen": "^5.0.0",
    "@ionic-native/status-bar": "^5.0.0",
    "@ionic/angular": "^4.7.1",
    "core-js": "^2.5.4",
    "rxjs": "~6.5.1",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1",
    "@ionic-native/camera": "5.13.0",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "ngx-webstorage": "4.0.1",
    "ng-jhipster": "0.11.1",
    "@fortawesome/angular-fontawesome": "0.5.0",
    "@fortawesome/fontawesome-svg-core": "1.2.22",
    "@fortawesome/free-solid-svg-icons": "5.10.2",
    "@ng-bootstrap/ng-bootstrap": "5.1.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.801.2",
    "@angular-devkit/build-angular": "~0.801.2",
    "@angular-devkit/core": "~8.1.2",
    "@angular-devkit/schematics": "~8.1.2",
    "@angular/cli": "~8.1.2",
    "@angular/compiler": "~8.1.2",
    "@angular/compiler-cli": "~8.1.2",
    "@angular/language-service": "~8.1.2",
    "@ionic/angular-toolkit": "~2.0.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "12.7.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "5.4.2",
    "ts-node": "8.3.0",
    "tslint": "~5.15.0",
    "typescript": "3.4.5",
    "generator-jhipster": "6.3.1",
    "@types/jest": "24.0.18",
    "html-loader": "0.5.5",
    "ionic-mocks-jest": "1.3.3",
    "istanbul-instrumenter-loader": "3.0.1",
    "jasmine": "3.4.0",
    "jest": "24.9.0",
    "jest-localstorage-mock": "2.4.0",
    "jest-preset-angular": "7.1.1",
    "null-loader": "3.0.0",
    "prettier": "1.18.2",
    "ts-loader": "6.0.4",
    "tslint-config-prettier": "1.18.0",
    "webdriver-manager": "12.1.6",
    "generator-jhipster-ionic": "4.2.0"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-camera": {
        "CAMERA_USAGE_DESCRIPTION": "Need camera access to take pictures",
        "PHOTOLIBRARY_USAGE_DESCRIPTION": "Need photo library access to get pictures from there"
      }
    }
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFiles": [
      "jest-localstorage-mock",
      "<rootDir>/src/setupTests.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "src/test.ts"
    ],
    "testURL": "http://localhost/",
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|@ionic-native|@ionic)"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/e2e/"
    ],
    "collectCoverageFrom": [
      "**/*.{ts}",
      "!**/*d.{ts}",
      "!**/*interface.{ts}",
      "!**/*module.{ts}",
      "!**/*main.{ts}",
      "!**/node_modules/**"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ],
    "coverageDirectory": "./public/coverage"
  },
  "description": "A hipster Ionic project, made with ðŸ’™ by @oktadev!"
}
